<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csat.shop.dao.GoodDetailDao">

    <select id="getGoodDetails" parameterType="Integer" resultType="com.csat.shop.entity.GoodDetail">
        select * from good_detail where product_id=#{productId}
