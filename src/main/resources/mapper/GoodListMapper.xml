<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csat.shop.dao.GoodListDao">

    <select id="getGoodsListData" resultType="com.csat.shop.entity.GoodsList">
        SELECT * FROM goods_list WHERE type_number=#{2} LIMIT #{0}, #{1}
    </select>

</mapper>