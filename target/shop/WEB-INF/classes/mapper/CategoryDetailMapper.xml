<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.csat.shop.dao.CategoryDetailDao">

    <select id="getCategoryDetailData" parameterType="String" resultType="com.csat.shop.entity.CategoryDetail">
        select *from category_detail where type_number=#{typeNumber}
    </select>

</mapper>